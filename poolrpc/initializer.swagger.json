{
  "swagger": "2.0",
  "info": {
    "title": "initializer.proto",
    "version": "version not set"
  },
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/v1/ping": {
      "get": {
        "operationId": "Ping",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/poolrpcPingResponse"
            }
          },
          "default": {
            "description": "An unexpected error response",
            "schema": {
              "$ref": "#/definitions/gatewayruntimeError"
            }
          }
        },
        "tags": [
          "Initializer"
        ]
      }
    },
    "/v1/pool/service/initialize": {
      "post": {
        "summary": "Tells poold to initialize itself, create a macaroon,\nand connect to LND.",
        "description": "If InitRequest.stateless_init is true, InitResponse\nwill contain raw bytes of poold.macaroon (which won't be saved to disk).\nOtherwise, InitResponse will simply return its success boolean.",
        "operationId": "InitializeService",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/poolrpcInitializeResponse"
            }
          },
          "default": {
            "description": "An unexpected error response",
            "schema": {
              "$ref": "#/definitions/gatewayruntimeError"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/poolrpcInitializeRequest"
            }
          }
        ],
        "tags": [
          "Initializer"
        ]
      }
    }
  },
  "definitions": {
    "gatewayruntimeError": {
      "type": "object",
      "properties": {
        "error": {
          "type": "string"
        },
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        },
        "details": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/protobufAny"
          }
        }
      }
    },
    "poolrpcInitializeRequest": {
      "type": "object",
      "properties": {
        "stateless_init": {
          "type": "boolean",
          "format": "boolean",
          "description": "Controls how poold initializes;\nif stateless_init is true, then poold.macaroon won't be saved to disk,\nand will instead be returned in InitResponse.\n\nOtherwise, poold.macaroon _will_ be saved to disk."
        },
        "lnd_auth_details": {
          "$ref": "#/definitions/poolrpcLndAuthDetails"
        }
      }
    },
    "poolrpcInitializeResponse": {
      "type": "object",
      "properties": {
        "init_succeeded": {
          "type": "boolean",
          "format": "boolean",
          "description": "Represents whether or not poold's initialization sequence succeeded.\nIf false, check the output of the `init_error` field."
        },
        "poold_macaroon": {
          "type": "string",
          "format": "byte",
          "description": "Raw byte data of the autogenerated poold.macaroon file.\n\nIf initiated with stateless_init set to false, this field will be empty,\nand poold.macaroon will instead be saved to\n$POOLD_DIR/$NETWORK/poold.macaroon."
        },
        "init_error": {
          "type": "string",
          "description": "If poold initialization failed for some reason, contains the error string\nof the initialization error."
        }
      }
    },
    "poolrpcLndAuthDetails": {
      "type": "object",
      "properties": {
        "host": {
          "type": "string",
          "description": "Host (whether a hostname or IP address) of the LND instance to connect to.\n\nMust not include the port number."
        },
        "port": {
          "type": "string",
          "format": "uint64",
          "description": "RPC port of the LND instance to connect to.\n\nDefaults to 10009 (LND's default RPC port)."
        },
        "admin_macaroon": {
          "type": "string",
          "description": "Base64-encoded data of LND's admin macaroon to authenticate with."
        },
        "tls_cert": {
          "type": "string",
          "description": "Base64-encoded data of LND's TLS certificate to authenticate with."
        }
      }
    },
    "poolrpcPingResponse": {
      "type": "object",
      "properties": {
        "pong": {
          "type": "string"
        }
      }
    },
    "protobufAny": {
      "type": "object",
      "properties": {
        "type_url": {
          "type": "string"
        },
        "value": {
          "type": "string",
          "format": "byte"
        }
      }
    }
  }
}
